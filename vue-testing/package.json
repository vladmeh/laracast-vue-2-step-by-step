{
  "name": "vue-testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/ava --color"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": ["env"],
    "plugins": ["transform-runtime"]
  },
  "ava": {
    "files": [
      "test/**/*"
    ],
    "helpers": [
      "**/helpers/**/*"
    ],
    "sources": [
      "src/**/*"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "_tap": true,
    "verbose": true,
    "compileEnhancements": false,
    "require": [
      "babel-register",
      "./test/helpers/_setup-browser-env.js"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false
      },
      "extensions": [
        "js",
        "jsx"
      ]
    }
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.6",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^3.0.0"
  },
  "dependencies": {
    "vue": "^2.6.10"
  }
}
